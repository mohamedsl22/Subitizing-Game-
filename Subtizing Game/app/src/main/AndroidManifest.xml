<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.moodisalman.subitizing">

    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_INTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/sushicon"
        android:label="@string/app_name"
        android:roundIcon="@drawable/sushicon"
        android:supportsRtl="false"
        android:theme="@style/AppTheme1">
        <activity android:name=".imageCrop"
            android:screenOrientation="portrait"></activity>
        <activity
            android:name="com.theartofdev.edmodo.cropper.CropImageActivity"
            android:theme="@style/AppThemeCrop" />
        <activity
            android:name=".infoLuncherActivity"
            android:screenOrientation="portrait"
            android:theme="@style/AppTheme1">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".choosegame" />
        <activity android:name=".Levels" />
        <activity
            android:name=".GameScreen"
            android:screenOrientation="landscape" />
        <activity android:name=".MainActivity"
            android:screenOrientation="portrait"></activity>
    </application>

</manifest>