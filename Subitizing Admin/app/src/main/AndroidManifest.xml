<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.moodisalman.subitizingadmin">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Pie_Chart"></activity>
        <activity android:name=".LineChart" />
        <activity
            android:name=".AllResults"
            android:label="Results list"
            android:theme="@style/AppTheme2" />
        <activity
            android:name=".Chart"
            android:theme="@style/AppTheme1" />
        <activity
            android:name=".UserFile"
            android:theme="@style/AppTheme1" />
        <activity
            android:name=".UsersList"
            android:label="Users list"
            android:theme="@style/AppTheme2" />
        <activity
            android:name=".AddUser"
            android:label="Add User" />
        <activity
            android:name=".MainActivity"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>